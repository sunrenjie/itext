<tagmap>
	<tag name="itext" alias="BODY" />
	<tag name="paragraph" alias="P" />
	<tag name="newline" alias="BR" />
	<tag name="phrase" alias="EM">
		<attribute name="style" value="italic" />
	</tag>
	<tag name="phrase" alias="STRONG">
		<attribute name="style" value="bold" />
	</tag>
	<tag name="anchor" alias="A">
		<attribute name="reference" alias="HREF" />
		<attribute name="name" alias="NAME" />
		<attribute name="red" value="0" />
		<attribute name="green" value="0" />
		<attribute name="blue" value="255" />
	</tag>
	<tag name="list" alias="UL">
		<attribute name="symbolindent" value="20" />
		<attribute name="listsymbol" value="o" />
	</tag>
	<tag name="listitem" alias="LI" />
	<tag name="chunk" alias="ADOBE" content="Adobe System Incorporated" />
</tagmap>