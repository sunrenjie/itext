<!ELEMENT itext (#PCDATA | newline | newpage | chunk | anchor | list | table | annotation | chapter | paragraph | phrase)*>
<!ATTLIST itext
	title		CDATA	#IMPLIED
	subject		CDATA	#IMPLIED
	keywords	CDATA	#IMPLIED
	author		CDATA	#IMPLIED
>

<!ELEMENT chunk (#PCDATA | newline | newpage)*> 
<!ATTLIST chunk
    font   CDATA   #IMPLIED
    size   CDATA   #IMPLIED
    style  CDATA   #IMPLIED
    color  CDATA   #IMPLIED
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    subsupscript        CDATA   #IMPLIED
    localgoto           CDATA   #IMPLIED
    localdestination    CDATA   #IMPLIED
    generictag          CDATA   #IMPLIED
>

<!ELEMENT phrase (#PCDATA | newline | newpage | chunk | anchor | list | table | annotation)*> 
<!ATTLIST phrase
    font   CDATA   #IMPLIED
    size   CDATA   #IMPLIED
    style  CDATA   #IMPLIED
    color  CDATA   #IMPLIED
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    leading    CDATA   #IMPLIED
>

<!ELEMENT anchor (#PCDATA | chunk | annotation | newline | newpage)*> 
<!ATTLIST anchor
    font   CDATA   #IMPLIED
    size   CDATA   #IMPLIED
    style  CDATA   #IMPLIED
    color  CDATA   #IMPLIED
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    leading    CDATA   #IMPLIED
    name       CDATA   #IMPLIED
    reference  CDATA   #IMPLIED
>

<!ELEMENT paragraph (#PCDATA | newline | newpage | chunk | anchor | list | table | annotation | phrase | image)*> 
<!ATTLIST paragraph
    font   CDATA   #IMPLIED
    size   CDATA   #IMPLIED
    style  CDATA   #IMPLIED
    color  CDATA   #IMPLIED
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    leading    CDATA   #IMPLIED
    align              CDATA   #IMPLIED
    indentationleft    CDATA   #IMPLIED
    indentationright   CDATA   #IMPLIED
>

<!ELEMENT list (listitem*)> 
<!ATTLIST list
    font   CDATA   #IMPLIED
    size   CDATA   #IMPLIED
    style  CDATA   #IMPLIED
    color  CDATA   #IMPLIED
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    indentationleft    CDATA   #IMPLIED
    indentationright   CDATA   #IMPLIED
    numbered       CDATA   #IMPLIED
    symbolindent   CDATA   #IMPLIED
    first          CDATA   #IMPLIED
    listsymbol     CDATA   #IMPLIED
>

<!ELEMENT listitem (#PCDATA | anchor | phrase | list | chunk | annotation | newline | newpage | image)*> 
<!ATTLIST listitem
    font   CDATA   #IMPLIED
    size   CDATA   #IMPLIED
    style  CDATA   #IMPLIED
    color  CDATA   #IMPLIED
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    leading    CDATA   #IMPLIED
    align              CDATA   #IMPLIED
    indentationleft    CDATA   #IMPLIED
    indentationright   CDATA   #IMPLIED
>

<!ELEMENT chapter (title?, sectioncontent) >
<!ATTLIST chapter
    depth              CDATA   #IMPLIED
    numberdepth        CDATA   #IMPLIED
    indent             CDATA   #IMPLIED
    indentationleft    CDATA   #IMPLIED
    indentationright   CDATA   #IMPLIED
>

<!ELEMENT section (title?, sectioncontent) >
<!ATTLIST section
    depth              CDATA   #IMPLIED
    numberdepth        CDATA   #IMPLIED
    indent             CDATA   #IMPLIED
    indentationleft    CDATA   #IMPLIED
    indentationright   CDATA   #IMPLIED
>

<!ELEMENT title (#PCDATA | newline | newpage | phrase | chunk | annotation)*>
<!ELEMENT sectioncontent (#PCDATA | newline | newpage | chunk | anchor | list | table | annotation | section | paragraph | phrase | image)*>

<!ELEMENT table (cell*)>
<!ATTLIST table
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    align              CDATA   #IMPLIED
    bgred              CDATA   #IMPLIED
    bggreen            CDATA   #IMPLIED
    bgblue             CDATA   #IMPLIED
    width              CDATA   #IMPLIED
    bordercolor        CDATA   #IMPLIED
    backgroundcolor    CDATA   #IMPLIED
    left               CDATA   #IMPLIED
    right              CDATA   #IMPLIED
    top                CDATA   #IMPLIED
    bottom             CDATA   #IMPLIED
    borderwidth        CDATA   #IMPLIED
    grayfill           CDATA   #IMPLIED
    columns        CDATA   #IMPLIED
    lastHeaderRow  CDATA   #IMPLIED
    cellpadding    CDATA   #IMPLIED
    cellspacing    CDATA   #IMPLIED
    widths         CDATA   #IMPLIED
>

<!ELEMENT row (cell*)>
<!ELEMENT cell (#PCDATA | paragraph | anchor | phrase | list | chunk | newline | newpage | table | image | annotation)*>
<!ATTLIST cell
    red    CDATA   #IMPLIED
    green  CDATA   #IMPLIED
    blue   CDATA   #IMPLIED
    leading    CDATA   #IMPLIED
    indentationleft    CDATA   #IMPLIED
    indentationright   CDATA   #IMPLIED
    bgred              CDATA   #IMPLIED
    bggreen            CDATA   #IMPLIED
    bgblue             CDATA   #IMPLIED
    width              CDATA   #IMPLIED
    bordercolor        CDATA   #IMPLIED
    backgroundcolor    CDATA   #IMPLIED
    left               CDATA   #IMPLIED
    right              CDATA   #IMPLIED
    top                CDATA   #IMPLIED
    bottom             CDATA   #IMPLIED
    borderwidth        CDATA   #IMPLIED
    grayfill           CDATA   #IMPLIED
    colspan    CDATA   #IMPLIED
    rowspan    CDATA   #IMPLIED
    header     CDATA   #IMPLIED
    nowrap     CDATA   #IMPLIED
>

<!ELEMENT image EMPTY>
<!ATTLIST image
    url             CDATA   #REQUIRED
    align           CDATA   #IMPLIED
    underlying      CDATA   #IMPLIED
    textwrap        CDATA   #IMPLIED
    alt             CDATA   #IMPLIED
    absolutex       CDATA   #IMPLIED
    absolutey       CDATA   #IMPLIED
    plainwidth      CDATA   #IMPLIED
    plainheight     CDATA   #IMPLIED
    rotation        CDATA   #IMPLIED
>

<!ELEMENT annotation EMPTY>
<!ATTLIST annotation
    title       CDATA   #IMPLIED
    content     CDATA   #IMPLIED
>

<!ELEMENT newpage EMPTY>
<!ELEMENT newline EMPTY>